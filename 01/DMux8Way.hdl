CHIP DMux8Way {
    IN in, sel[3];
    OUT a, b, c, d, e, f, g, h;

    PARTS:
    DMux4Way(in=in, sel=sel[1..2], a=outa, b=outb, c=outc, d=outd);

	DMux(in=outa,sel=sel[0],a=a,b=b);
	DMux(in=outb,sel=sel[0],a=c,b=d);
	DMux(in=outc,sel=sel[0],a=e,b=f);
	DMux(in=outd,sel=sel[0],a=g,b=h);
}
